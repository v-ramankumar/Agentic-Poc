# Preauth Agent API Environment Configuration
# Copy this file to .env and update with your actual values

# Database Configuration
MONGODB_URL=mongodb://localhost:27017
DATABASE_NAME=preauth_agent_db

# External Service URLs
N8N_WEBHOOK_URL=http://localhost:5678/webhook/preauth
AGENT_URL=http://localhost:8000/agent/process
PATIENT_API_URL=http://localhost:9000/api
BASE_URL=http://localhost:8001

# API Configuration
API_VERSION=1.0.0
DEBUG=True

# Security (Optional - implement if needed)
API_KEY=your-secure-api-key
JWT_SECRET=your-jwt-secret-key

# CORS Configuration
ALLOWED_ORIGINS=["http://localhost:3000", "http://localhost:5173", "http://localhost:8080"]

# N8N Configuration
N8N_API_KEY=your-n8n-api-key
N8N_BASE_URL=http://localhost:5678

# External Patient Data Service
PATIENT_SERVICE_API_KEY=your-patient-service-key
PATIENT_SERVICE_TIMEOUT=30

# File Storage (for screenshots, documents)
BLOB_STORAGE_URL=https://your-storage-account.blob.core.windows.net
BLOB_CONTAINER_NAME=preauth-documents

# Notification Services (Optional)
EMAIL_SERVICE_URL=http://localhost:7000/email
SMS_SERVICE_URL=http://localhost:7001/sms

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE_PATH=./logs/preauth_agent.log

# Performance Settings
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT=300
CLEANUP_INTERVAL_HOURS=24
